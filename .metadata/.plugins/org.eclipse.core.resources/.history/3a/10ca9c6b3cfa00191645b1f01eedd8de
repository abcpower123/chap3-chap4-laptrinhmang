package ex1;

import java.io.InputStream;
import java.io.OutputStream;
import java.net.Socket;

public class Client {

	public static void main(String args[]){
		try {
			Socket s = new Socket("localhost",123);		// Kết nối đến Server
			
			InputStream is = s.getInputStream();	// Lấy InputStream 
			OutputStream os = s.getOutputStream(); 	// Lấy OutputStream 
			os.write("123321".getBytes());
			s.close();
		} catch(Exception ie){
			System.out.println("Loi: Khong tao duoc socket");
			ie.printStackTrace();
		}	
	}	


}
